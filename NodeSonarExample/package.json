{
  "name": "NodeSonarExample",
  "scripts": {
    "sonar": "node sonar-project.js",
    "test": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
